{
  "name": "sqltools-cassandra",
  "displayName": "Cassandra for SQLTools",
  "description": "Cassandra Driver for SQLTools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "mtxr",
  "license": "MIT",
  "repository": {},
  "bugs": {},
